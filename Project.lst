				; --------------------------------------------------------------------------------- 
				; Name: validateString
				;  
				; Description: This procedure checks each character in the argument string to ensure
				;				it is valid. If it is not, the error flag is set.
				; 
				; Preconditions: Argument addresses should be valid. Data at addresses should be initialized.
				; 
				; Postconditions: Registers are restored after procedure call.
				; 
				; Receives: 
				;			[EBP + 24] -> Address offset of where user string input is saved.
				;			[EBP + 28] -> Address offset of where the user input string length is saved.
				;			[EBP + 32] -> Address offset of where the input error flag should be saved.
				;			[EBP + 36] -> Address offset of where the sign of user input should be saved.
				;
				; Returns: The following data may be changed after this procedure:
				;			[EBP + 32] -> Address offset of where the input error flag is saved.
				;			[EBP + 36] -> Address offset of where the sign of user input is saved.
				; ---------------------------------------------------------------------------------
 0000014A			validateString PROC
				  ; get a Stack reference point
 0000014A  55			  PUSH  EBP
 0000014B  8B EC		  MOV   EBP, ESP

				  ; set up loop
 0000014D  8B 75 08		  MOV	ESI, [EBP+8]
 00000150  8B 4D 0C		  MOV	ECX, [EBP+12]

				  ; if invalid
 00000153  83 F9 00		  CMP	ECX, 0
 00000156  7E 3E		  JLE	_inputLengthError
 00000158  83 F9 0B		  CMP	ECX, 11
 0000015B  7D 39		  JGE	_inputLengthError

 0000015D  B8 00000000		  MOV	EAX, 0
 00000162  FC			  CLD
 00000163  AC			  LODSB

				  ; check for sign
 00000164  FF 75 14		  PUSH	[EBP+20]
 00000167  FF 75 10		  PUSH	[EBP+16]
 0000016A  50			  PUSH	EAX
 0000016B  E8 00000033		  CALL	validateFirstCharacter

				  ;decrement ECX and verify
 00000170  49			  DEC	ECX
 00000171  83 F9 00		  CMP	ECX, 0
 00000174  7E 29		  JLE	_validateEnd

 00000176			_nextCharacter:

				  ; clear EAX and startt next digit
 00000176  B8 00000000		  MOV	EAX, 0
 0000017B  FC			  CLD
 0000017C  AC			  LODSB

				  ; validate
 0000017D  FF 75 10		  PUSH	[EBP+16]
 00000180  50			  PUSH	EAX
 00000181  E8 00000052		  CALL	validateCharacter

				  ; break loop if invalid
 00000186  B8 00000000		  MOV	EAX, 0
 0000018B  8B 55 10		  MOV	EDX, [EBP+16]
 0000018E  3B 02		  CMP	EAX, [EDX]
 00000190  75 0D		  JNE	_validateEnd

 00000192  E2 E2		  LOOP	_nextCharacter

 00000194  EB 09		  JMP	_validateEnd

 00000196			_inputLengthError:
 00000196  8B 45 10		  MOV	EAX, [EBP+16]
 00000199  C7 00 00000001	  MOV	DWORD PTR [EAX], 1

 0000019F			_validateEnd:
 0000019F  5D			  POP	EBP
 000001A0  C2 0010		  RET	16

 000001A3			validateString ENDP

				; --------------------------------------------------------------------------------- 
				; Name: validateFirstCharacter 
				;  
				; Description: This procedure validates the first character of a user string input.
				;				It allows characters that are +, -, or numerical inputs (in ASCII).
				;				If the character is not valid, the error flag is set.
				;				If a negative sign is found, the sign flag is set to -1.
				; 
				; Preconditions: Argument addresses should be valid. Data at addresses should be initialized.
				; 
				; Postconditions: Registers are restored after procedure call.
				; 
				; Receives: [EBP + 16] -> ASCII character byte (hexadecimal)
				;			[EBP + 20] -> Address offset of where the input error flag should be saved.
				;			[EBP + 24] -> Address offset of where the sign of the user input should be saved.
				;
				; Returns: The following data may be changed after this procedure:
				;			[EBP + 20] -> Address offset of where the input error flag is saved.
				;			[EBP + 24] -> Address offset of where the sign of the user input is saved.
				; ---------------------------------------------------------------------------------
 000001A3			validateFirstCharacter PROC

				  ; get a Stack reference point
 000001A3  55			  PUSH  EBP
 000001A4  8B EC		  MOV   EBP, ESP

 000001A6  8B 45 08		  MOV	EAX, [EBP+8]


 000001A9  83 F8 2B		  CMP	EAX, 43d
 000001AC  72 26		  JB	_errorFirstCharEnd
 000001AE  83 F8 2D		  CMP	EAX, 45d
 000001B1  74 0C		  JE	_minusSign

				  ; check if first value is number
 000001B3  83 F8 30		  CMP	EAX, 48d
 000001B6  72 13		  JB	_errorFirstChar
 000001B8  83 F8 39		  CMP	EAX, 57d
 000001BB  77 0E		  JA	_errorFirstChar
 000001BD  EB 15		  JMP	_errorFirstCharEnd

 000001BF			_minusSign:
				  ; set sign flag to neg 1
 000001BF  8B 45 10		  MOV	EAX, [EBP+16]
 000001C2  BA FFFFFFFF		  MOV	EDX, -1
 000001C7  89 10		  MOV	[EAX], EDX
 000001C9  EB 09		  JMP	_errorFirstCharEnd

 000001CB			_errorFirstChar:
 000001CB  8B 45 0C		  MOV	EAX, [EBP+12]
 000001CE  C7 00 00000001	  MOV	DWORD PTR [EAX], 1

 000001D4			_errorFirstCharEnd:
 000001D4  5D			  POP	EBP
 000001D5  C2 000C		  RET 12

 000001D8			validateFirstCharacter ENDP

				; --------------------------------------------------------------------------------- 
				; Name: validateCharacter 
				;  
				; Description: This procedure validates a character of a user string input.
				;				It allows characters that are numerical inputs (in ASCII).
				;				If the character is not valid, the error flag is set.
				; 
				; Preconditions: Argument addresses should be valid. Data at addresses should be initialized.
				; 
				; Postconditions: Registers are restored after procedure call.
				; 
				; Receives: [EBP + 12] -> ASCII character byte (hexadecimal)
				;			[EBP + 16] -> Address offset of where the input error flag should be saved.
				;
				; Returns: The following data may be changed after this procedure:
				;			[EBP + 16] -> Address offset of where the input error flag is saved.
				; ---------------------------------------------------------------------------------
 000001D8			validateCharacter PROC
				  ; get a Stack reference point
 000001D8  55			  PUSH  EBP
 000001D9  8B EC		  MOV   EBP, ESP

 000001DB  8B 45 08		  MOV	EAX, [EBP+8]

				  ; check if character is numeric
 000001DE  83 F8 30		  CMP	EAX, 48d
 000001E1  72 07		  JB	_error
 000001E3  83 F8 39		  CMP	EAX, 57d
 000001E6  77 02		  JA	_error
 000001E8  EB 09		  JMP	_errorEnd

 000001EA			_error:
 000001EA  8B 45 0C		  MOV	EAX, [EBP+12]
 000001ED  C7 00 00000001	  MOV	DWORD PTR [EAX], 1

 000001F3			_errorEnd:
 000001F3  5D			  POP	EBP
 000001F4  C2 0008		  RET	8

 000001F7			validateCharacter ENDP

				; --------------------------------------------------------------------------------- 
				; Name: stringToSDWORD 
				;  
				; Description: This procedure converts a string to a signed double word (SDWORD) value.
				;				If the value is too large, the error flag is set.
				; 
				; Preconditions: Argument addresses should be valid. Data at addresses should be initialized.
				; 
				; Postconditions: Registers are restored after procedure call.
				; 
				; Receives:
				;			[EBP + 32] -> Address offset of where user string input should be saved.
				;			[EBP + 36] -> Address offset of where the user input string length should be saved.
				;			[EBP + 40] -> Address offset of where the input error flag should be saved.
				;			[EBP + 44] -> Address offset of where the sign of user input should be saved.
				;			[EBP + 48] -> Address offset of where the converted SDWORD value should be saved.
				;
				; Returns: The following data may be changed after this procedure:
				;			[EBP + 40] -> Address offset of where the input error flag is saved.
				;			[EBP + 48] -> Address offset of where the converted SDWORD value is saved.
				; ---------------------------------------------------------------------------------
 000001F7			stringToInteger PROC
				  ; get a Stack reference point
 000001F7  55			  PUSH  EBP
 000001F8  8B EC		  MOV   EBP, ESP

				  ; prepare registers	
 000001FA  8B 75 08		  MOV	ESI, [EBP+8]
 000001FD  8B 7D 18		  MOV	EDI, [EBP+24]
 00000200  8B 4D 0C		  MOV	ECX, [EBP+12]
 00000203  BB 00000000		  MOV	EBX, 0
 00000208  BA 00000001		  MOV	EDX, 1

				  ; ESI to last
 0000020D  8B C1		  MOV	EAX, ECX
 0000020F  48			  DEC	EAX
 00000210  03 F0		  ADD	ESI, EAX

